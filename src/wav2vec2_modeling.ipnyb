","        ctc_loss = ctc_loss_fn(log_probs, encoded_texts, input_lengths, target_lengths)\n","\n","\n","\n","        loss = ctc_loss\n","        # loss = nn.Softmax(dim=-1)(loss)\n","        total_loss += loss.item()\n","        total_samples += p.size(0)  # 배치 크기 추가\n","\n","        loss.backward()\n","        optimizer.step()\n","\n","    # 에폭의 평균 손실 계산\n","    avg_train_loss = total_loss / len(train_loader)\n","    train_losses.append(avg_train_loss)  # 평균 손실 리스트에 추가\n","\n","    # 검증 단계\n","    model.eval()  # 모델을 평가 모드로 설정\n","    total_val_loss = 0\n","\n","\n","    predictions = []\n","    references = []\n","\n","\n","    with torch.no_grad():\n","        for val_batch in tqdm(val_loader):\n","            val_p = val_batch['input_values'].to('cuda' if torch.cuda.is_available() else 'cpu')\n","            val_encoded_texts = val_batch['labels'].to('cuda' if torch.cuda.is_available() else 'cpu')\n","\n","\n","            # Step 1: encoded_texts를 텐서에서 문자열로 변환\n","            decoded_texts = processor.tokenizer.batch_decode(val_encoded_texts)\n","\n","            # Step 2: 문자열에서 '볍'과 '[unk]' 제거\n","            processed_texts = [t.replace('볍', '').replace('[unk]', '') for t in decoded_texts]\n","\n","            # Step 3: 다시 텐서로 변환 (토크나이저를 사용하여 인코딩)\n","            val_encoded_texts = processor.tokenizer(processed_texts, return_tensors='pt', padding=True).input_ids\n","\n","            # cuda로 다시 이동\n","            val_encoded_texts = val_encoded_texts.to('cuda' if torch.cuda.is_available() else 'cpu')\n","\n","            val_outputs = model(val_p, labels=val_encoded_texts)\n","\n","            logits = val_outputs.logits.log_softmax(dim=-1)\n","            # encoded_texts = tokenizer(texts, return_tensors=\"pt\", padding=True).input_ids\n","            ctc_loss_fn = torch.nn.CTCLoss()\n","\n","            # CTC 손실 계산\n","            log_probs = logits.transpose(0, 1)  # (sequence_length, batch_size, vocab_size)\n","            input_lengths = torch.full(size=(logits.size(0),), fill_value=logits.size(1), dtype=torch.long)  # 입력 시퀀스 길이\n","            target_lengths = torch.tensor([len(t) for t in val_encoded_texts])  # 타겟 시퀀스 길이\n","\n","            ctc_loss = ctc_loss_fn(log_probs, val_encoded_texts, input_lengths, target_lengths)\n","\n","            val_loss = ctc_loss\n","\n","            # val_loss = nn.Softmax(dim=-1)(val_loss)\n","            total_val_loss += val_loss.item()\n","\n","\n","            # 예측값 추출\n","            logits = val_outputs.logits\n","            pred_ids = torch.argmax(logits, dim=-1)\n","\n","            # 예측값과 실제 라벨을 디코딩 (토크나이저 필요)\n","            predictions.extend([t.replace('볍','') for t in tokenizer.batch_decode(pred_ids, skip_special_tokens=True)])  # 예측값 디코딩\n","            references.extend([t.replace('볍','') for t in tokenizer.batch_decode(val_encoded_texts, skip_special_tokens=True)])  # 실제 라벨 디코딩\n","\n","    #wer, cer\n","    print(predictions, references)\n","    wer = wer_metric.compute(predictions=predictions, references=references)\n","    cer = cer_metric.compute(predictions=predictions, references=references)\n","    wer_values.append(wer)\n","    cer_values.append(cer)\n","\n","    avg_val_loss = total_val_loss / len(val_loader)\n","    val_losses.append(avg_val_loss)\n","\n","    #earlystopping\n","    if i == 0:\n","        best_val_loss = avg_val_loss\n","        patience_counter = 0\n","    else:\n","        if avg_val_loss < best_val_loss:\n","            best_val_loss = avg_val_loss\n","            patience_counter = 0\n","        else:\n","            patience_counter += 1\n","            if patience_counter >= patient:\n","                print(f\"Early stopping triggered at epoch {i + 1}.\")\n","                break\n","\n","    # 에폭 메시지 출력\n","    print(f\"\\nEpoch {i + 1}/{epochs}, Average Train Loss: {avg_train_loss:.4f}, Average Val Loss: {avg_val_loss:.4f}, WER: {wer:.4f}, CER: {cer:.4f}\")"],"metadata":{"id":"qd3OMKVfVX0a"},"execution_count":null,"outputs