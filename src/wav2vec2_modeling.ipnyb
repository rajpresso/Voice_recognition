 speech, _ = sf.read(audio_path)\n","\n","        # 각 오디오 파일에 해당하는 레이블 파일 경로 생성\n","        label_file = audio_file.replace('.wav', '.txt')  # 오디오 파일 이름을 기반으로 레이블 파일 이름 생성\n","        label_path = os.path.join(self.label_dir, label_file)\n","\n","        # 레이블 파일 읽기\n","        with open(label_path, 'r', encoding='utf-8') as f:\n","            transcription = f.read().strip()  # 텍스트 내용 읽기\n","\n","        transcription.replace(' ','')\n","        # print(transcription)\n","\n","\n","        # Wav2Vec2 입력 형식으로 변환\n","        inputs = self.processor(speech, sampling_rate=16000, return_tensors=\"pt\", padding=\"longest\")\n","\n","        # 입력값과 레이블 반환\n","        return {\n","            'input_values': inputs.input_values.squeeze(0),  # [1, time] 형태에서 [time]으로 변환\n","            'labels': transcription\n","        }\n","\n","# Wav2Vec2 Tokenizer 초기화\n","tokenizer = Wav2Vec2Processor.from_pretrained(\"kresnik/wav2vec2-large-xlsr-korean\").tokenizer\n","\n","# DataLoader에서 사용할 collate_fn 정의\n","def collate_fn(batch):\n","    input_values = [item['input_values'] for item in batch]\n","    labels = [item['labels'] for item in batch]\n","\n","    # input_values 패딩\n","    input_values = torch.nn.utils.rnn.pad_sequence(input_values, batch_first=True)\n","\n","    # 레이블 인덱스 변환\n","    label_ids = tokenizer(labels, padding=True, return_tensors=\"pt\").input_ids  # 텍스트를 인덱스로 변환\n","\n","    return {\n","        'input_values': input_values,\n","        'labels': label_ids\n","    }\n","\n","# Wav2Vec2 프로세서 초기화\n","processor = Wav2Vec2Processor.from_pretrained(\"kresnik/wav2vec2-large-xlsr-korean\")\n","\n","\n","audio_dir = \"/content/audio\"\n","label_dir = \"/content/label\"\n","\n","dataset = CustomAudioDataset(audio_dir, label_dir, processor)\n","\n","# 전체 데이터셋 (이미 생성된 dataset 객체를 사용)\n","dataset_size = len(dataset)\n","train_size = int(0.9 * dataset_size)  # 70% for training\n","val_size = int(0.05 * dataset_size)   # 15% for validation\n","test_size = dataset_size - train_size - val_size  # 15% for testing\n","batch_size = 8\n","\n","# 데이터셋 분할\n","train_dataset, val_dataset, test_dataset = random_split(dataset, [train_size, val_size, test_size])\n","\n","train_loader = DataLoader(train_dataset, batch_size=batch_size, shuffle=True, collate_fn=collate_fn)\n","val_loader = DataLoader(val_dataset, batch_size=batch_size, shuffle=False, collate_fn=collate_fn)\n","test_loader = DataLoader(test_dataset, batch_size=batch_size, shuffle=False, collate_fn=collate_fn)\n","\n","# 분할된 데이터셋 크기 확인\n","print(f\"Training set size: {train_size}\")\n","print(f\"Validation set size: {val_size}\")\n","print(f\"Test set size: {test_size}\")"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"ljgj3tyrUq5_"},"outputs":[],"source":[]},{"cell_type":"markdown","metadata":{"id":"hvn20U20UYfL"},"source":["#4.Modeling 및 학습"]},{"cell_type":"code","execution_count":20,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":160,"referenced_widgets":["0734d0f4b8f248ae8622b7bec16f6d2c","e69a712081dc4f8fb45b2e7c4c1df975","63c1dad981fc4d4691709df8b6385e40","c2ccb6333e4a435d9b46cdeb8d71f80e","3c25777a6ce746d9bab1452ecab46eae","8407cea1fea5467ab1eb3dd2f99318be","31538ef88dd44dc8af23ff2670f3c3e8","ee21f82bc22e4f1bb12e41258654c52f","08b68e047ee84ccda54eeac180443254","670d4f6509b843d583850cb28153cc20","8c79d4938b4541fda2792d6ebdea10e2"]},"executionInfo":{"elapsed":4919,"status":"ok","timestamp":1728263288443,"user":{"displayName":"음성인식","userId":"16492850060121368281"},"user_tz":-540},"id":"Zl7yMoWg23wb","outputId":"c6328aab-2c34-4b16-ad0f-d7e629a723de"},"outputs":[{"output_type":"display_data","data":{"text/plain":["model.safetensors:   0%|          | 0.00/1.27G [00:00<?, ?B/s]"],"application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"0734d0f4b8f248ae8622b7bec16f6d2c"}},"metadata":{}},{"output_type":"stream","name":"stderr","text":["Some weights of the model checkpoint at kresnik/wav2vec2-large-xlsr-korean were not